struct Test {
  var a: Int
  var b: Bool

  func bar() {
    a += 2
  }

  func baz() -> Int {
    return a
  }
}

contract A {
  var a: Int
  var test: Test
  var test2: Test
}

A :: (any) {
  mutating public func modifyTest() {
    test.bar()
  }

  mutating public func modifyTest2() {
    test2.bar()
  }

  public func getTest() -> Int {
    return test.baz()
  }

  public func getTest2() -> Int {
    return test2.baz()
  }
}
