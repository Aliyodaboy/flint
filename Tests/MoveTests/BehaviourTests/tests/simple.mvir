import Transaction.Simple;

main () {
  let this: address;
  this = get_txn_sender();
  Simple.publish();
  assert(Simple.getCount(copy(this)) == 0, 1);
  Simple.increment(copy(this));
  assert(Simple.getCount(copy(this)) == 1, 2);
  Simple.increment$Int(copy(this), 4);
  assert(Simple.getCount(copy(this)) == 5, 3);
  Simple.reset(copy(this));
  assert(Simple.getCount(copy(this)) == 0, 4);
  return;
}
