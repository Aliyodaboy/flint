struct Rectangle {
  public var width: Int = 0
  public var height: Int = 0

  public init(width: Int, height: Int) {
    self.width = width
    self.height = height
  }
}

contract myContract {
  var rectangle: Rectangle
}

myContract :: (any) {
  public init() {
    rectangle = Rectangle()
  }

  public init(rectangle: Rectangle) {
    self.rectangle = rectangle
  }

  public area(): Int {
    return rectangle.width * rectangle.height
  }
}
