contract Counter {
  var value: Int = 10
  var backup: Int = 0
}

Counter :: (any) {
  public init() {
    backup = 10
  }

  public func getValue() -> Int {
    return value
  }

  public func increment()
    mutates (value)
  {
    value += 1
  }

  public func set(value: Int)
    mutates (value)
  {
    self.value = value
  }
}
