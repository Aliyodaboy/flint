contract Wallet {
  var owner: Address
  var contents: Wei = 0 
}

Wallet :: caller <- (any) {
  public init() {
    owner = caller
  }

  public mutating func deposit(money: Wei) {
    contents += money
  }
}

Wallet :: (owner) {
  public mutating func withdraw(money: Wei) {
    if money > contents {
      contents -= money
    }
  }

  public func getContents(a: Int) -> Wei {
    return contents
  }
}
