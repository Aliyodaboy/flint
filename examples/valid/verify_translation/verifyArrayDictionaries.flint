contract Array {
  var as: [Int]
  var bs: [Int: Int]

  var cs: [[Int]]
  var ds: [[[Int]]]// = []
  var es: [Int: [[Int]]]// = [:]
}

Array :: (any) {
  public init() {
    as = []
    bs = [:]
    cs = []
    ds = []
    es = [:]
  }

  func arrayLiteral()
    mutates (as, bs, cs)
  {
    as[0] = 0
    bs[10] = 5
    cs[0] = []
    cs[0][0] = 0

    //let b: [[Int]] = []
  }

  func arrayDictSize() {
    let asSize: Int = as.size
    let bsSize: Int = bs.size
  }

  func dictKeys() {
    let bsKeys: [Int] = bs.keys

    let esKeys: [Int] = es.keys
    let esSubSize: Int = es[esKeys[0]].size
  }

  func growSize() {
    var as: [Int] = [1,2,3,4]
    let as_size: Int = as.size
    as[4] = 5
    assert(as_size + 1 == as.size)
  }
}
